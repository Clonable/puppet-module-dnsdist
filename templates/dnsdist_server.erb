newServer({
    address='<%= @address %>',
<% if @source -%>
    source='<%= @source %>',
<% end -%>
<% if @qps -%>
    qps=<%= @qps %>,
<% end -%>
<% if @weight -%>
    weight=<%= @weight %>,
<% end -%>
<% if @pool -%>
    pool='<%= @pool %>',
<% end -%>
<% if @extra_options -%>
<% @extra_options.each do |option, value| -%>
    <%= option %>='<%= value %>',
<% end -%>
<% end -%>
})
